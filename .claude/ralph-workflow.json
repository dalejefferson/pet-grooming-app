{
  "name": "Ralph Workflow",
  "version": "1.0",
  "workflows": {
    "default": [
      "install_deps.sh",
      "implement",
      "lint_fix.sh",
      "run_tests.sh",
      "build.sh",
      "commit_changes.sh"
    ],
    "quick": [
      "implement",
